		#else
			<div id="comments" class="item" style="clear:both;">
				<span style="color:red; font-weight:bold; display:none;" id="systemMessage"></span>
				
				#if($subscription.error)
					#if($subscription.errorCode == 201 || $subscription.errorCode == 200 ||$subscription.errorCode == 204 || $subscription.errorCode == 203 || $subscription.errorCode == 205)
						#set($errorMessage = "Sorry, but that e-mail was not found.")
					#else
						#set($errorMessage = "Sorry, but there was a problem sending your password reminder.")
					#end
					
					<script>
						RegActions.displayMessage({
							hideFormName: "forgotPasswordForm",
							errorMessage: "$errorMessage"
						});
					</script>	
				#end				
				
				<form action="/s" name="reset" method="post">
							<input type="hidden" name="action" value="requestPasswordReset">
							<input type="hidden" name="subject" value="Animal Bites Password Reset">
							<input type="hidden" name="rurl" value="$rurl">
					<p>
						<label for="email_address"><span class="bold">Email Address:</span><br>
							<input type="text" name="email" id="emailInput" size="50" maxlength="128">

						</label>
					</p>
					<p>
						<input type="submit" name="submit" id="submit" value="Reset Password">
					</p>
						</form>
