$cms.template("Top")

<script type="text/javascript">
	function validateForgotPasswordForm(form) {
		try{
			RegActions.displayMessage({
				hideFormName: "forgotPasswordForm",
				errorMessage: "Processing Request...",
				disableAction: "none"
			});

			var errorMessages = [];

			if(form.elements["email"].value == "")
				errorMessages.push("You must enter your email.");
			else if(!ValidateFormFunctions.isValidEmail(form.elements["email"].value))
				errorMessages.push("Invalid Email Address.");

			if(errorMessages.length > 0){
               	RegActions.displayMessage({
					hideFormName: "forgotPasswordForm",
					errorMessage: 'The following errors were encountered:<br/><ul><li>' + errorMessages.join('</li><li>') + '</li></ul>'
				});
				return false;
			}
		}catch(e){ 
			RegActions.displayMessage({
				hideFormName: "forgotPasswordForm",
				errorMessage: e,
				errorType: "System",
			});
		}
	}
</script>

<div id="newsarticle">
	<div class="bodyCopy">
		#if($req.param("action") == "requestPassword" && !$subscription.error)
			<p>Thank you.  A password reset link has been sent to your email.</p>
